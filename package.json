{
  "name": "parquetjs",
  "description": "fully asynchronous, pure JavaScript implementation of the Parquet file format",
  "main": "parquet.js",
  "version": "0.11.2",
  "homepage": "https://github.com/ironSource/parquetjs",
  "author": "ironSource <npm@ironsrc.com>",
  "license": "MIT",
  "keywords": [
    "dremel",
    "parquet"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ironSource/parquetjs.git"
  },
  "dependencies": {
    "async-mutex": "^0.5.0",
    "brotli": "^1.3.3",
    "bson": "^6.10.4",
    "fast-zstd": "^1.0.5",
    "int53": "^1.0.0",
    "object-stream": "0.0.1",
    "snappyjs": "^0.7.0",
    "thrift": "^0.22.0",
    "varint": "^6.0.0"
  },
  "optionalDependencies": {
    "lzo": "^0.4.11"
  },
  "devDependencies": {
    "chai": "^6.0.1",
    "mocha": "^11.7.2",
    "nyc": "^17.1.0"
  },
  "scripts": {
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "coverage": "nyc mocha && nyc report --reporter=html"
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "parquet.js"
    ],
    "exclude": [
      "gen-nodejs/**",
      "test/**",
      "examples/**"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "cache": false,
    "all": true
  },
  "engines": {
    "node": ">=7.6"
  }
}
